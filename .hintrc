{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "accent-color",
          "backdrop-filter",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "disown-opener": "off",
    "apple-touch-icons": "off"
  }
}